{
  "name": "ulka-language-support",
  "displayName": "Ulka Language Support",
  "description": "Syntax highlight for .ulka files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.47.0"
  },
  "publisher": "Roshan",
  "author": {
    "name": "Roshan Acharya",
    "email": "acharyaroshan2357@gmail.com"
  },
  "icon": "images/logo.png",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ulkajs/vscode-ulka-language-support.git"
  },
  "bugs": {
    "url": "https://github.com/ulkajs/vscode-ulka-language-support/issues"
  },
  "homepage": "https://github.com/ulkajs/vscode-ulka-language-support#readme",
  "license": "MIT",
  "configurationDefaults": {
    "emmet.includeLanguages": {
      "ulka": "html"
    },
    "material-icon-theme.files.associations": {
      "*.ulka": "html"
    }
  },
  "contributes": {
    "languages": [
      {
        "id": "ulka",
        "aliases": [
          "ulka",
          "Ulka"
        ],
        "extensions": [
          ".ulka"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ulka",
        "scopeName": "source.ulka",
        "path": "./syntaxes/ulka.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript"
        }
      },
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "js-html.injection",
        "injectTo": [
          "source.ulka"
        ]
      }
    ]
  }
}
